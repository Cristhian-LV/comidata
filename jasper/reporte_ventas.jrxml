	<queryString language="SQL">
		<![CDATA[SELECT
 c.nombres,
 strftime('%d/%m/%Y %H:%M:%S', v.fecha_gener / 1000, 'unixepoch', 'localtime') AS fecha_formateada,
 v.*
FROM upeu_venta v
 JOIN ss_cliente c ON c.dniruc = v.dniruc
WHERE date(v.fecha_gener / 1000, 'unixepoch', 'localtime')
 BETWEEN date( $P{fechaI} )
 AND date( $P{fechaF} )]]>
	</queryString>
